    "@types/react-dom": "^19.0.3",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "10.4.20",
    "gh-pages": "6.3.0",
    "postcss": "8.5.2",
    "prettier": "3.4.2",
    "tailwindcss": "4.0.2",
    "typescript": "5.7.3"
  }
