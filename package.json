  "homepage": "https://milliorn.github.io/grocery-list",
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "react-scripts": "5.0.1",
    "sweetalert2": "11.10.5",
    "uuid": "9.0.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
