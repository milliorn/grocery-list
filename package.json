  "homepage": "https://milliorn.github.io/grocery-list",
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "5.4.0",
    "react-scripts": "5.0.1",
    "sweetalert2": "11.15.10",
    "uuid": "11.0.5"
  },
  "scripts": {
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
