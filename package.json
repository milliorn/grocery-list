    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "10.4.17",
    "gh-pages": "6.1.1",
    "postcss": "8.4.34",
    "prettier": "3.2.5",
    "tailwindcss": "3.4.1",
    "typescript": "4.9.5"
