  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "react-scripts": "5.0.1",
    "sweetalert2": "11.14.5",
    "uuid": "11.0.3"
  },
  "scripts": {
    "build": "react-scripts build",
